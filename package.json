    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:critical": "npm audit --audit-level=critical",
    "security:audit:fix": "npm audit fix",
    "security:audit:report": "npm audit --extensions ts --json > security-report.json",
    "madge": "madge --extensions ts src",
    "madge:circular": "madge --extensions ts src --circular",
    "analyze:graph": "madge --extensions ts src --image madge-graph.svg",
    "analyze:orphans": "madge src --orphans",
    "analyze:circular": "madge --extensions ts src --circular",
    "analyze:deps:json": "madge --extensions ts src --json src/ > madge-report.json",
    "analyze:deps": "madge --image deps.png src/",
    "analyze:all": "npm run analyze:circular && npm run analyze:deps:json && npm run analyze:complexity"
