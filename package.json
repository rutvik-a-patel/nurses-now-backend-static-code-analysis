    "migration:generate": "export NODE_ENV=localhost && npm run typeorm -- migration:generate",
    "migration:up:localhost": "export NODE_ENV=localhost && npm run typeorm -- migration:run",
    "migration:down:localhost": "export NODE_ENV=localhost && npm run typeorm -- migration:revert",
    "migration:up:development": "export NODE_ENV=development && npm run typeorm -- migration:run",
    "migration:down:development": "export NODE_ENV=development && npm run typeorm -- migration:revert",
    "migration:up:staging": "export NODE_ENV=staging && npm run typeorm -- migration:run",
    "migration:down:staging": "export NODE_ENV=staging && npm run typeorm -- migration:revert",
    "migration:up:prod": "export NODE_ENV=production && npm run typeorm -- migration:run",
    "migration:down:prod": "export NODE_ENV=production && npm run typeorm -- migration:revert",
