    "pre:commit": "npm run format && npm run lint",
    "pre:push": "npm run build",
    "pm2:localhost": "export NODE_ENV=localhost && npm run build && pm2 start dist/src/main.js -n NursesNowBackend",
    "pm2:start": "export NODE_ENV=development && npm run build && pm2 start dist/src/main.js --name NursesNowBackend -i 2",
    "pm2:prod": "export NODE_ENV=production && npm run build && pm2 start dist/src/main.js --name NURSESNOWBACKENDPROD -i MAX",
    "pm2:reload": "export NODE_ENV=development && npm run build && pm2 reload NursesNowBackend",
    "pm2:start:stage": "export NODE_ENV=staging && npm run build && pm2 start dist/src/main.js --name NursesNowBackendStage -i 2",
    "pm2:reload:stage": "export NODE_ENV=staging && npm run build && pm2 reload NursesNowBackendStage",
    "pm2:reload:prod": "export NODE_ENV=production && npm run build && pm2 reload NURSESNOWBACKENDPROD",
